<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <LinearLayout
        android:id="@+id/main"
        android:layout_width="match_parent"
        android:layout_height="1094dp"
        android:orientation="vertical"
        tools:context=".Food_Search_Activity">


        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="50dp"
            android:textSize="10pt"
            android:text="서치 결과"
            android:layout_gravity="center"
            android:fontFamily="@font/maplestorybold"
            />

        <LinearLayout
            android:id="@+id/lay1Id"
            android:layout_width="match_parent"
            android:layout_marginTop="30dp"
            android:layout_height="180dp"
            android:orientation="vertical"
            >

            <TextView
                android:id="@+id/titleTextView1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="15pt"
                android:text="Title 1"
                android:fontFamily="@font/maplestorybold"
                />

            <TextView
                android:id="@+id/linkTextView1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:textSize="10pt"
                android:textColor="#0000FF"
                android:text="Link 1"
                android:fontFamily="@font/maplestorybold"
                />
            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_marginTop="30dp"
                tools:context=".Activities.JoinActivity">

                <ImageView
                    android:id="@+id/imageTextView430"
                    android:layout_width="150dp"
                    android:layout_height="100dp"
                    android:layout_marginLeft="30dp"
                    android:src="@android:mipmap/sym_def_app_icon" />

                <ImageView
                    android:id="@+id/imageTextView1"
                    android:layout_width="150dp"
                    android:layout_height="100dp"
                    android:layout_marginLeft="40dp"
                    android:layout_toRightOf="@+id/imageTextView430"
                    android:src="@android:mipmap/sym_def_app_icon" />
            </RelativeLayout>

        </LinearLayout>
        <LinearLayout
            android:id="@+id/lay2Id"
            android:layout_width="match_parent"
            android:layout_marginTop="30dp"
            android:layout_height="170dp"
            android:orientation="vertical"
            >

            <TextView
                android:id="@+id/titleTextView2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="15pt"
                android:text="Title 2"
                android:fontFamily="@font/maplestorybold"
                />

            <TextView
                android:id="@+id/linkTextView2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:textSize="10pt"
                android:textColor="#0000FF"
                android:text="Link 2"
                android:fontFamily="@font/maplestorybold"
                />
            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_marginTop="30dp"
                tools:context=".Activities.JoinActivity">

                <ImageView
                    android:id="@+id/imageTextView431"
                    android:layout_width="150dp"
                    android:layout_height="100dp"
                    android:layout_marginLeft="30dp"
                    android:src="@android:mipmap/sym_def_app_icon" />

                <ImageView
                    android:src="@android:mipmap/sym_def_app_icon"
                    android:id="@+id/imageTextView2"
                    android:layout_width="150dp"
                    android:layout_height="100dp"
                    android:layout_marginLeft="40dp"
                    android:layout_toRightOf="@+id/imageTextView431"
                    />

            </RelativeLayout>

        </LinearLayout>

        <LinearLayout
            android:id="@+id/lay3Id"
            android:layout_width="match_parent"
            android:layout_marginTop="30dp"
            android:layout_height="180dp"
            android:orientation="vertical"
            >
            <TextView
                android:id="@+id/titleTextView3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="15pt"
                android:text="Title 3"
                android:fontFamily="@font/maplestorybold"/>


            <TextView
                android:id="@+id/linkTextView3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="8dp"
                android:textSize="10pt"
                android:textColor="#0000FF"
                android:text="Link 3"
                android:fontFamily="@font/maplestorybold"/>

            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_marginTop="30dp"
                tools:context=".Activities.JoinActivity">
                <ImageView
                    android:id="@+id/imageTextView439"
                    android:layout_width="150dp"
                    android:layout_height="100dp"
                    android:layout_marginLeft="30dp"
                    android:src="@android:mipmap/sym_def_app_icon" />

                <ImageView
                    android:src="@android:mipmap/sym_def_app_icon"
                    android:id="@+id/imageTextView3"
                    android:layout_width="150dp"
                    android:layout_height="100dp"
                    android:layout_marginLeft="30dp"
                    android:layout_toRightOf="@+id/imageTextView439"/>
            </RelativeLayout>
        </LinearLayout>

        <Button
            android:id="@+id/backButton"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:text="뒤로 가기"
            android:layout_gravity="center"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/linkTextView3"
            android:fontFamily="@font/maplestorybold"/>


    </LinearLayout>


</layout>
